<TASK LASTMODSTRING="2/01/2007 (4:23:27 PM)" RISK="0" STARTDATESTRING="2/01/2007" PRIORITYCOLOR="22446" NUMPERSON="1" PRIORITY="10" LASTMOD="39084.68295139" TITLE="Typical Registry Entries Supporting File Associations" CREATIONDATESTRING="2/01/2007" CREATIONDATE="39084.00000000" PERSON="Brad" COMMENTS="[HKEY_CLASSES_ROOT\.tdl]
